<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.smarttoolfactory.projectboilerplate.MainFragment"
        android:label="MainFragment"
        tools:layout="@layout/fragment_main"/>


    <!-- dynamic feature module-->
    <include-dynamic
        android:id="@+id/nav_graph_dynamic"
        android:name="com.smarttoolfactory.dynamicfeature"
        app:graphResName="nav_graph_dynamic"
        app:moduleName="dynamicfeature">
    </include-dynamic>
</navigation>